-- Roblox LocalScript

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- Variable to store the BillboardGui name tag references
local nameTags = {}

-- Function to create a BillboardGui with player's name above their head
local function createNameTag(player)
    local character = player.Character
    if not character then return end

    -- Ensure the character has a head
    local head = character:FindFirstChild("Head")
    if not head then return end

    -- Create BillboardGui
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Adornee = head
    billboardGui.Parent = head
    billboardGui.Size = UDim2.new(0, 200, 0, 50)
    billboardGui.StudsOffset = Vector3.new(0, 3, 0)
    billboardGui.AlwaysOnTop = true

    -- Create TextLabel
    local textLabel = Instance.new("TextLabel")
    textLabel.Parent = billboardGui
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.TextColor3 = Color3.new(1, 1, 1)
    textLabel.TextStrokeTransparency = 0.5
    textLabel.Text = player.Name
    textLabel.TextScaled = true

    -- Store reference for later removal
    nameTags[player] = billboardGui
end

-- Function to remove name tags
local function removeNameTags()
    for player, billboardGui in pairs(nameTags) do
        if player.Character and player.Character:FindFirstChild("Head") then
            billboardGui:Destroy()
        end
    end
    nameTags = {}
end

-- Create name tags for players who join the game
Players.PlayerAdded:Connect(createNameTag)

-- Remove name tags when players leave the game
Players.PlayerRemoving:Connect(function(player)
    if nameTags[player] then
        nameTags[player]:Destroy()
        nameTags[player] = nil
    end
end)

-- Function to teleport to a specific player
local function teleportTo(targetPlayer)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        player.Character:MoveTo(targetPlayer.Character.HumanoidRootPart.Position)
        print("Teleporting to " .. targetPlayer.Name)
    else
        print("Target player is invalid or not found.")
    end
end

-- Function to find the nearest player
local function getNearestPlayer()
    local nearestPlayer = nil
    local shortestDistance = math.huge
    
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            local distance = (player.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).magnitude
            if distance < shortestDistance then
                shortestDistance = distance
                nearestPlayer = p
            end
        end
    end
    
    return nearestPlayer
end

-- Function to find the farthest player
local function getFarthestPlayer()
    local farthestPlayer = nil
    local longestDistance = 0
    
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            local distance = (player.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).magnitude
            if distance > longestDistance then
                longestDistance = distance
                farthestPlayer = p
            end
        end
    end
    
    return farthestPlayer
end

-- Function to reset the player's character
local function resetCharacter()
    if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        humanoid.Health = 0  -- Set health to 0 to effectively reset the character
        player.Character:BreakJoints()  -- Ensure the character is properly reset
        print("Character has been reset.")
    else
        print("Humanoid not found in the character.")
    end
end

-- Function to tween to a specific player
local function tweenTo(targetPlayer, speed)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
        local startPosition = player.Character.HumanoidRootPart.Position
        
        local distance = (startPosition - targetPosition).magnitude
        local tweenTime = distance / (speed or 50)  -- Default speed is 50 if not provided
        
        local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local tweenGoal = { CFrame = CFrame.new(targetPosition) }
        
        local tween = TweenService:Create(player.Character.HumanoidRootPart, tweenInfo, tweenGoal)
        tween:Play()
        tween.Completed:Connect(function()
            print("Tweening to " .. targetPlayer.Name .. " completed at " .. (speed or 50) .. " studs per second.")
        end)
    else
        print("Target player is invalid or not found.")
    end
end

-- Function to handle chat messages
local function onPlayerChatted(message)
    local words = string.split(message, " ")

    if words[1] == "+m" then
        -- Create a ScreenGui
        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "MyScreenGui"
        screenGui.Parent = game.CoreGui

        -- Create TextBox for Sound ID
        local soundIdBox = Instance.new("TextBox")
        soundIdBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        soundIdBox.BackgroundTransparency = 0.8
        soundIdBox.BorderColor3 = Color3.fromRGB(255, 0, 0)
        soundIdBox.BorderSizePixel = 2
        soundIdBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        soundIdBox.Font = Enum.Font.PatrickHand
        soundIdBox.TextScaled = true
        soundIdBox.Size = UDim2.new(0.1, 0, 0.1, 0)
        soundIdBox.Position = UDim2.new(0.3, 0, -0.1, 0)
        soundIdBox.PlaceholderText = "Custom SoundID: 15689451063"
        soundIdBox.Text = ""
        soundIdBox.ClearTextOnFocus = true
        soundIdBox.Parent = screenGui

        -- Create UI Corner for Sound ID TextBox
        local idBoxCorner = Instance.new("UICorner")
        idBoxCorner.CornerRadius = UDim.new(0.2, 0)
        idBoxCorner.Parent = soundIdBox

        -- Create TextBox for Pitch
        local pitchBox = Instance.new("TextBox")
        pitchBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        pitchBox.BackgroundTransparency = 0.8
        pitchBox.BorderColor3 = Color3.fromRGB(0, 255, 0)
        pitchBox.BorderSizePixel = 2
        pitchBox.TextColor3 = Color3.fromRGB(255, 255, 0)
        pitchBox.Font = Enum.Font.PatrickHand
        pitchBox.TextScaled = true
        pitchBox.Size = UDim2.new(0.05, 0, 0.1, 0)
        pitchBox.Position = UDim2.new(0.41, 0, -0.1, 0)
        pitchBox.PlaceholderText = "Pitch value from 0 to 3"
        pitchBox.Text = ""
        pitchBox.ClearTextOnFocus = true
        pitchBox.Parent = screenGui

        -- Create UI Corner for Pitch TextBox
        local pitchBoxCorner = Instance.new("UICorner")
        pitchBoxCorner.CornerRadius = UDim.new(0.2, 0)
        pitchBoxCorner.Parent = pitchBox

        -- Create a ToggleButton for Looping
        local loopToggleButton = Instance.new("TextButton")
        loopToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        loopToggleButton.BackgroundTransparency = 0.8
        loopToggleButton.BorderColor3 = Color3.fromRGB(0, 0, 255)
        loopToggleButton.BorderSizePixel = 2
        loopToggleButton.TextColor3 = Color3.fromRGB(127, 0, 255)
        loopToggleButton.Font = Enum.Font.PatrickHand
        loopToggleButton.TextScaled = true
        loopToggleButton.Size = UDim2.new(0.05, 0, 0.1, 0)
        loopToggleButton.Position = UDim2.new(0.47, 0, -0.1, 0)
        loopToggleButton.Text = "ON"
        loopToggleButton.Parent = screenGui

        -- Create UI Corner for Loop ToggleButton
        local loopButtonCorner = Instance.new("UICorner")
        loopButtonCorner.CornerRadius = UDim.new(0.2, 0)
        loopButtonCorner.Parent = loopToggleButton

        -- Create a Sound instance in the SoundService
        local sound = Instance.new("Sound")
        sound.Parent = game:GetService("SoundService")

        -- Variable to track loop status
        local isLooping = false

        -- Function to update sound based on TextBox inputs
        local function updateSound()
            local soundId = tonumber(soundIdBox.Text)
            if soundId then
                if sound.IsPlaying then
                    sound:Stop()
                end
                sound.SoundId = "rbxassetid://" .. soundId
                sound:Play()
            end
        end

        -- Function to update pitch
        local function updatePitch()
            local pitch = tonumber(pitchBox.Text)
            if pitch and pitch > 0 and pitch <= 3 then
                sound.PlaybackSpeed = pitch
            end
        end

        -- Function to toggle looping
        local function toggleLoop()
            isLooping = not isLooping
            sound.Looped = isLooping
            if isLooping and not sound.IsPlaying then
                sound:Play()
            end
        end

        -- Update sound when Enter is pressed in Sound ID TextBox
        soundIdBox.FocusLost:Connect(function(enterPressed)
            if enterPressed then
                updateSound()
            end
        end)

        -- Update pitch when Enter is pressed in Pitch TextBox
        pitchBox.FocusLost:Connect(function(enterPressed)
            if enterPressed then
                updatePitch()
            end
        end)

        -- Toggle looping when the ToggleButton is clicked
        loopToggleButton.MouseButton1Click:Connect(function()
            toggleLoop()
            loopToggleButton.Text = isLooping and "OFF" or "ON"
        end)
        
    elseif words[1] == "+nom" then
        -- Find and remove the ScreenGui named "MyScreenGui"
        local screenGui = game.CoreGui:FindFirstChild("MyScreenGui")
        if screenGui then
            screenGui:Destroy()
        end
  elseif words[1] == "+t" then
    if #words == 2 then
        if words[2] == "r" then
            local allPlayers = Players:GetPlayers()
            local otherPlayers = {}
            
            for _, p in pairs(allPlayers) do
                if p ~= player then
                    table.insert(otherPlayers, p)
                end
            end
            
            if #otherPlayers > 0 then
                local randomPlayer = otherPlayers[math.random(#otherPlayers)]
                teleportTo(randomPlayer)
            else
                print("No other players to teleport to.")
            end
        elseif words[2] == "n" then
            local nearestPlayer = getNearestPlayer()
            if nearestPlayer then
                teleportTo(nearestPlayer)
            else
                print("No players found.")
            end
        elseif words[2] == "f" then
            local farthestPlayer = getFarthestPlayer()
            if farthestPlayer then
                teleportTo(farthestPlayer)
            else
                print("No players found.")
            end
        else
            local targetPlayer = Players:FindFirstChild(words[2])
            if targetPlayer then
                teleportTo(targetPlayer)
            else
                print("Player with username " .. words[2] .. " not found.")
            end
        end
    else
        print("Invalid teleport command.")
    end
