local ScreenGui = Instance.new("ScreenGui")
local Frame1 = Instance.new("Frame")
local TextButton1 = Instance.new("TextButton")
local Frame2 = Instance.new("Frame")
local TextButton2 = Instance.new("TextButton")
local Frame3 = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local TeleportButton = Instance.new("TextButton")

ScreenGui.Parent = game.CoreGui

-- Frame1 and TextButton1
Frame1.Parent = ScreenGui
Frame1.BackgroundColor3 = Color3.new(0,0,0)
Frame1.BackgroundTransparency = 0.5
Frame1.BorderColor3 = Color3.new(0,0,1)
Frame1.BorderSizePixel = 2
Frame1.Position = UDim2.new(0.7, 0, 0.491666675, 0)
Frame1.Size = UDim2.new(0.1,0.1,0.1)
Frame1.Active = true
Frame1.Draggable = true

TextButton1.Parent = Frame1
TextButton1.BackgroundColor3 = Color3.new(1,1,1)
TextButton1.BackgroundTransparency = 0.5
TextButton1.Position = UDim2.new(0.103524067, 0, 0.200333327, 0)
TextButton1.TextColor3 = Color3.new(0,0,0)
TextButton1.Size = UDim2.new(0.8,0.9,0.6)
TextButton1.Font = Enum.Font.SourceSansLight
TextButton1.FontSize = Enum.FontSize.Size14
TextButton1.Text = "Collect Coins"
TextButton1.TextScaled = true
TextButton1.TextSize = 8
TextButton1.TextWrapped = true

TextButton1.MouseButton1Click:Connect(function()
    local Coin = game:GetService("Workspace").Normal.CoinContainer.Coin_Server
    if Coin then
        print("hi")
    else
        print("Coin has been collected")
    end

    task.wait(0.1)

    local Player = game.Players.LocalPlayer
    local Hum = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")

    if Hum then
        local Part = Instance.new("Part")
        Part.Parent = Workspace
        Part.Name = "Spot"
        Part.Anchored = true
        Part.Color = Color3.new(0,1,1)
        Part.Position = Hum.Position
        Part.Shape = Enum.PartType.Ball
        Part.Transparency = 1
        Part.CanCollide = false

        Frame1.BackgroundColor3 = Color3.new(1,0,0)
        TextButton1.Text = "Now Collect"
        Frame1.Active = false

        task.wait(0.1)
        Player.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Normal.CoinContainer.Coin_Server.CFrame

        task.wait(0.1)

        local Part0 = Instance.new("Part")
        Part0.Parent = Workspace
        Part0.Name = "Check0"
        Part0.Anchored = true
        Part0.Shape = Enum.PartType.Ball
        Part0.Transparency = 0.6
        Part0.CanCollide = false
        Part0.Position = Hum.Position
        Part0.Color = Color3.new(0,1,0)

        local Part1 = Instance.new("Part")
        Part1.Parent = Workspace
        Part1.Name = "Check"
        Part1.Anchored = true
        Part1.Shape = Enum.PartType.Ball
        Part1.Position = Hum.Position + Vector3.new(0,0,4)
        Part1.Transparency = 0.6
        Part1.CanCollide = false
        Part1.Color = Color3.new(1,0,0)

        task.wait(0.2)
        Player.Character.HumanoidRootPart.CFrame = game.Workspace.Check.CFrame

        task.wait(0.2)
        Player.Character.HumanoidRootPart.CFrame = game.Workspace.Check0.CFrame

        task.wait(0.2)
        Player.Character.HumanoidRootPart.CFrame = game.Workspace.Spot.CFrame

        Frame1.BackgroundColor3 = Color3.new(0,0,0)
        TextButton1.Text = "Collect Coins"
        Frame1.Active = true

        task.wait(0.1)
        for _, part in ipairs(Workspace:GetChildren()) do
            if part:IsA("Part") and (part.Name == "Spot" or part.Name == "Check0" or part.Name == "Check") then
                part:Destroy()
            end
        end
    end
end)

-- Frame2 and TextButton2
Frame2.Parent = ScreenGui
Frame2.BackgroundColor3 = Color3.new(0,0,0)
Frame2.BackgroundTransparency = 0.5
Frame2.BorderColor3 = Color3.new(0,0,1)
Frame2.BorderSizePixel = 2
Frame2.Position = UDim2.new(0.8, 0, 0.491666675, 0)
Frame2.Size = UDim2.new(0.1,0.1,0.1)
Frame2.Active = true
Frame2.Draggable = true

TextButton2.Parent = Frame2
TextButton2.BackgroundColor3 = Color3.new(1,1,1)
TextButton2.BackgroundTransparency = 0.5
TextButton2.Position = UDim2.new(0.103524067, 0, 0.200333327, 0)
TextButton2.TextColor3 = Color3.new(0,0,0)
TextButton2.Size = UDim2.new(0.8,0.9,0.6)
TextButton2.Font = Enum.Font.SourceSansLight
TextButton2.FontSize = Enum.FontSize.Size14
TextButton2.Text = "Tp-GunDrop"
TextButton2.TextScaled = true
TextButton2.TextSize = 8
TextButton2.TextWrapped = true

TextButton2.MouseButton1Click:Connect(function()
    local GunDrop = game:GetService("Workspace").Normal.GunDrop
    if GunDrop then
        print("hi")
    else
        print("GunDrop found!")
    end

    task.wait(0.1)

    local Player = game.Players.LocalPlayer
    local Hum = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")

    if Hum then
        local Part = Instance.new("Part")
        Part.Parent = Workspace
        Part.Name = "Spot1"
        Part.Anchored = true
        Part.Color = Color3.new(0,1,1)
        Part.Position = Hum.Position
        Part.Shape = Enum.PartType.Ball
        Part.Transparency = 1
        Part.CanCollide = false

        Frame2.BackgroundColor3 = Color3.new(1,0,0)
        TextButton2.Text = "In Progress..."
        Frame2.Active = false

        task.wait(0.1)
        Player.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Normal.GunDrop.CFrame

        task.wait(0.1)
        Player.Character.HumanoidRootPart.CFrame = game.Workspace.Spot1.CFrame

        Frame2.BackgroundColor3 = Color3.new(0,0,0)
        TextButton2.Text = "Tp-GunDrop"
        Frame2.Active = true

        task.wait(0.1)
        if workspace:FindFirstChild("Spot1") then
            workspace.Spot1:Destroy()
        end
    end
end)

-- Frame3, TextBox, and TeleportButton
Frame3.Parent = ScreenGui
Frame3.BackgroundColor3 = Color3.new(0,0,0)
Frame3.BackgroundTransparency = 0.5
Frame3.BorderColor3 = Color3.new(0,0,1)
Frame3.BorderSizePixel = 2
Frame3.Position = UDim2.new(0.9, 0, 0.491666675, 0)
Frame3.Size = UDim2.new(0.1,0.1,0.1)
Frame3.Active = true
Frame3.Draggable = true

TextBox.Parent = Frame3
TextBox.BackgroundColor3 = Color3.new(1,1,1)
TextBox.BackgroundTransparency = 0.5
TextBox.Position = UDim2.new(0.1, 0, 0.2, 0)
TextBox.Size = UDim2.new(0.8, 0, 0.4, 0)
TextBox.TextColor3 = Color3.new(0,0,0)
TextBox.Font = Enum.Font.SourceSans
TextBox.TextSize = 14
TextBox.PlaceholderText = "Enter username"

TeleportButton.Parent = Frame3
TeleportButton.BackgroundColor3 = Color3.new(1,1,1)
TeleportButton.BackgroundTransparency = 0.5
TeleportButton.Position = UDim2.new(0.1, 0, 0.6, 0)
TeleportButton.Size = UDim2.new(0.8, 0, 0.4, 0)
TeleportButton.TextColor3 = Color3.new(0,0,0)
TeleportButton.Font = Enum.Font.SourceSansLight
TeleportButton.FontSize = Enum.FontSize.Size14
TeleportButton.Text = "Teleport Player"
TeleportButton.TextScaled = true
TeleportButton.TextSize = 8
TeleportButton.TextWrapped = true

TeleportButton.MouseButton1Click:Connect(function()
    local username = TextBox.Text
    local playerToTeleport = game.Players:FindFirstChild(username)
    
    if playerToTeleport then
        local Player = game.Players.LocalPlayer
        local Hum = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
        local targetHum = playerToTeleport.Character and playerToTeleport.Character:FindFirstChild("HumanoidRootPart")

        if Hum and targetHum then
            Hum.CFrame = targetHum.CFrame
            print("Teleported to " .. username)
        else
            print("Failed to teleport")
        end
    else
        print("Player not found")
    end
end)




local tweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:WaitForChild("PlayerGui")  -- Parent ScreenGui to PlayerGui

local function showWelcomeMessage()
    -- Create a new TextButton for the welcome message
    local welcomeTextButton = Instance.new("TextButton")
    welcomeTextButton.Parent = screenGui
    welcomeTextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    welcomeTextButton.BackgroundTransparency = 0.5
    welcomeTextButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
    welcomeTextButton.BorderSizePixel = 2
    welcomeTextButton.Position = UDim2.new(0.5, -100, 0.5, -50)  -- Center of the screen
    welcomeTextButton.Size = UDim2.new(0, 200, 0, 100)
    welcomeTextButton.Text = "Welcome to my script! | Script by Acient make for fun | How To Use: Collect Coins button will teleport you to the coins if it exist, same with Tp-GunDrop button only work if someone dropped a gun. There is another button you need to writed inside the text box correct username any players exist and press Teleport ! | I'm new.. GUI or Script still have Update | Tap this GUI to make it disappear ! BYE"
    welcomeTextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    welcomeTextButton.Font = Enum.Font.PatrickHand
    welcomeTextButton.TextScaled = true
    welcomeTextButton.TextWrapped = true

    local uiCornerWelcomeTextButton = Instance.new("UICorner")
    uiCornerWelcomeTextButton.CornerRadius = UDim.new(0.5, 0)
    uiCornerWelcomeTextButton.Parent = welcomeTextButton

    local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
    local goal = {TextTransparency = 1}
    local tween = tweenService:Create(welcomeTextButton, tweenInfo, goal)

    local function onClick()
        tween:Play()
        tween.Completed:Connect(function()
            welcomeTextButton:Destroy()
        end)
    end

    welcomeTextButton.MouseButton1Click:Connect(onClick)
end

showWelcomeMessage()
