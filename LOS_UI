local function finishRaceCollection(teleportSpeed)
    local partName = "finishPart"

    local function findParts(partName, parent)
        local parts = {}
        for _, obj in pairs(parent:GetChildren()) do
            if obj:IsA("Model") or obj:IsA("Folder") then
