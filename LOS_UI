ToggleButton.Parent = ScreenGui
ToggleButton.BackgroundColor3 = Color3.new(0, 0, 0)
ToggleButton.BackgroundTransparency = 0.5
ToggleButton.Size = UDim2.new(0.05, 0, 0.1, 0)
ToggleButton.TextScaled = true
ToggleButton.Position = UDim2.new(0.88, 0, -0.1, 0)
ToggleButton.TextColor3 = Color3.new(1, 1, 1)
ToggleButton.Font = Enum.Font.PatrickHand
ToggleButton.Text = "+"

local ToggleButtonCorner = Instance.new("UICorner")
ToggleButtonCorner.Parent = ToggleButton
ToggleButtonCorner.CornerRadius = UDim.new(0, 10)

local TweenService = game:GetService("TweenService")
local tweenSpeed = 1
local tweenInfo = TweenInfo.new(tweenSpeed, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut)

local goalPosition1 = UDim2.new(0.695, 0, 0.18, 0)
local goalPosition2 = UDim2.new(1.2, 0, 0.18, 0)

local tween1 = TweenService:Create(ScrollingFrame, tweenInfo, {Position = goalPosition1})
local tween2 = TweenService:Create(ScrollingFrame, tweenInfo, {Position = goalPosition2})

local isToggled = false
ToggleButton.MouseButton1Click:Connect(function()
    if isToggled then
        tween2:Play()
    else
        tween1:Play()
    end
    isToggled = not isToggled
end)

-- Button ON and OFF --

local button1 = Instance.new("TextButton")
button1.Parent = ScrollingFrame
button1.BackgroundColor3 = Color3.new(0, 0, 0)
button1.BackgroundTransparency = 0.5
button1.Size = UDim2.new(0.03, 0, 0.9, 0)
button1.TextScaled = true
button1.Position = UDim2.new(0.005, 0, 0, 0)
button1.TextColor3 = Color3.fromRGB(255, 255, 255)
button1.Font = Enum.Font.PatrickHand
button1.Text = "Get Speed Orbs: OFF"

local loopRunning = false
local teleportSpeed = 0.01 -- Default teleport speed, can be modified

button1.MouseButton1Click:Connect(function()
    if loopRunning then
        button1.Text = "Get Speed Orbs: OFF"
        button1.TextColor3 = Color3.fromRGB(255, 255, 255)
        loopRunning = false
    else
        button1.Text = "Get Speed Orbs: ON"
        button1.TextColor3 = Color3.fromRGB(0, 255, 0)
        loopRunning = true
        
        while loopRunning do
            outerOrbCollection(teleportSpeed)
            wait(0.01)
        end
    end
end)

