local ScreenGui = Instance.new("ScreenGui")
local ToggleButton = Instance.new("TextButton")
local ScrollingFrame = Instance.new("ScrollingFrame")
local ScrollingS = Instance.new("ScrollingFrame")
local ScrollingI = Instance.new("ScrollingFrame")

ScreenGui.Parent = game.CoreGui

-- Function to collect and teleport to all outerOrb parts --

local function outerOrbCollection(teleportSpeed)
    local partName = "outerOrb"

    local function findParts(partName, parent)
        local parts = {}
        for _, obj in pairs(parent:GetChildren()) do
            if obj:IsA("Model") or obj:IsA("Folder") then
                local foundParts = findParts(partName, obj)
                for _, part in pairs(foundParts) do
                    table.insert(parts, part)
                end
            elseif obj:IsA("BasePart") and obj.Name == partName then
                table.insert(parts, obj)
            end
        end
        return parts
    end

    local function teleportPlayerToPart(player, part)
        if player and part and part:IsA("BasePart") then
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = part.CFrame
            end
        end
    end

    local player = game.Players.LocalPlayer
    local parts = findParts(partName, game.Workspace)

    if #parts > 0 then
        for _, part in ipairs(parts) do
            teleportPlayerToPart(player, part)
            wait(teleportSpeed) -- Wait before teleporting to the next part
        end
    else
        local CoreGui = game:GetService("StarterGui")
        CoreGui:SetCore("SendNotification", {
            Title = "Error",
            Text = "There are no more Orbs to collect!",
            Duration = 2,
        })
    end
end

